{
    "family": "fargate-task-def-next-1",
    "executionRoleArn": "arn:aws:iam::007555096130:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "next-communiteam-fargate",
            "image": "007555096130.dkr.ecr.eu-west-1.amazonaws.com/next-communiteam:latest",
            "environmentFiles": [
                {
                  "value": "arn:aws:s3:::s3-next-1/.env",
                  "type": "s3"
                }],
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024"
}